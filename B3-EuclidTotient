//Giai bai toan tim gia tri phi Euler
#include <iostream>
using namespace std;

int main() {
    long long p = 857504083339712752489993810777LL;
    long long q = 1029224947942998075080348647219LL;
    
    long long phiEuler = (p - 1) * (q - 1);

    cout << phiEuler << endl;

    return 0;
}

