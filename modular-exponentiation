int main (){
    int a, m;
    int p = 17, q = 23;
    long long e = 65537;
    m = p*q;
    cout << "so can nhap la: ";
    cin >> a;
    long long s = MOD(a, e, m);
    cout << "so can tim la: " << s << endl;
    return 0;
}

